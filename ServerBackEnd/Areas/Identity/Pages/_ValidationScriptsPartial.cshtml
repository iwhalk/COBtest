<environment include="Development">
    <script src="~/Identity/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/Identity/lib/jquery-validation/dist/jquery.validate.js"></script>
    @* <script src="https://cdn.tailwindcss.com"></script> *@
