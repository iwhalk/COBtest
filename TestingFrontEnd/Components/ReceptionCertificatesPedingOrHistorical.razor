<div id="ContenedorActas">
    <div id="TituloActas" class="flex justify-center">
        <label for="" class="text-4xl text-blue-900 font-semibold my-8">Acta Entrega Recepción de Inmueble Histórico</label>
    </div>
    <div id="DatosInputs" class="flex justify-between mx-2 mb-2 px-4 pt-2 pb-4 rounded-lg" style="background-color: #E3F2F6;">
        <span class="flex flex-col justify-center">
            <label for="" class="mb-1 text-3xl text-center text-cyan-700">Fechas</label>
            <div>
                <input type="date" class="px-2 mx-2 py-1 border-1 border-black rounded-lg">
                <input type="date" class="px-2 mx-2 py-1 border-1 border-black rounded-lg">
            </div>
        </span>
        <span class="flex flex-col items-center">
            <label for="" class="mb-1 text-3xl text-cyan-700">Tipo de Acta</label>
            <select name="" class="px-2 py-1 border-1 border-black rounded-lg w-36 flex justify-center"></select>
        </span>
        <span class="flex flex-col items-center">
            <label for="" class="mb-1 text-3xl text-cyan-700">Tipo de Inmueble</label>
            <select name="" class="px-2 py-1 border-1 border-black rounded-lg w-36">
                @if (PropertyTypes is not null)
                {
                    @foreach (var item in PropertyTypes)
                    {
                        <option>@item.PropertyTypeName</option>
                    }
                }
            </select>
        </span>
        <span class="flex flex-col items-center">
            <label for="" class="mb-1 text-3xl text-cyan-700">No. Habitaciones</label>
            <input type="number" class="px-2 py-1 border-1 border-black rounded-lg w-36">
        </span>
        <span class="flex flex-col items-center">
            <label for="" class="mb-1 text-3xl text-cyan-700">Arrendador</label>
            <select name="" class="px-2 py-1 border-1 border-black rounded-lg w-36">
                @if (Lessors is not null)
                {
                    @foreach (var item in Lessors)
                    {
                        <option>@item.Name</option>
                    }
                }
            </select>
        </span>
        <span class="flex flex-col items-center">
            <label for="" class="mb-1 text-3xl text-cyan-700">Arrendatario</label>
            <select name="" class="px-2 py-1 border-1 border-black rounded-lg w-36">
                @if (Tenants is not null)
                {
                    @foreach (var item in Tenants)
                    {
                        <option>@item.Name</option>
                    }
                }
            </select>
        </span>
        <span class="flex flex-col items-center">
            <label for="" class="mb-1 text-3xl text-cyan-700">Delegación</label>
            <select name="" class="px-2 py-1 border-1 border-black rounded-lg w-36">
                <option>Álvaro Obregón</option>
                <option>Azcapotzalco</option>
                <option>Benito Juárez</option>
                <option>Coyoacán</option>
                <option>Cuajimalpa de Morelos</option>
                <option>Cuauhtémoc</option>
                <option>Gustavo A. Madero</option>
                <option>Iztacalco</option>
                <option>Iztapalapa</option>
                <option>Magdalena Contreras</option>
                <option>Miguel Hidalgo</option>
                <option>Milpa Alta</option>
                <option>Tláhuac</option>
                <option>Tlalpan</option>
                <option>Venustiano Carranza</option>
                <option>Xochimilco</option>
            </select>
        </span>
        <span class="flex flex-col items-center">
            <label for="" class="mb-1 text-3xl text-cyan-700">Agente</label>
            <select name="" class="px-2 py-1 border-1 border-black rounded-lg w-36"></select>
        </span>
    </div>
    <div id="Actas" class="flex justify-around mt-4">
        <div class="relative" style="height: 35rem">
            <table>
                <thead>
                    <tr>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal">
                        </th>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal" style="background-color: #E3F2F6;">
                            Fecha
                        </th>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal" style="background-color: #E3F2F6;">
                            Acta
                        </th>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal" style="background-color: #E3F2F6;">
                            Inmueble
                        </th>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal" style="background-color: #E3F2F6;">
                            Habitaciones
                        </th>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal" style="background-color: #E3F2F6;">
                            Arrendador
                        </th>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal" style="background-color: #E3F2F6;">
                            Arrendatario
                        </th>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal" style="background-color: #E3F2F6;">
                            Ciudad
                        </th>
                        <th class="py-2 px-12 text-center mb-1 text-3xl text-center text-cyan-700 font-normal" style="background-color: #E3F2F6;">
                            Agente
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @if (Actas is not null)
                    {
                        @foreach (var item in Actas)
                        {
                            <tr>
                                <td class="py-2 text-center text-2xl">
                                    @if (TypeTableReception == "Historico")
                                    {
                                        <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" fill="currentColor" class="bi bi-lock-fill" viewBox="0 0 16 16">
                                            <path d="M8 1a2 2 0 0 1 2 2v4H6V3a2 2 0 0 1 2-2zm3 6V3a3 3 0 0 0-6 0v4a2 2 0 0 0-2 2v5a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V9a2 2 0 0 0-2-2z" />
                                        </svg>
                                    }
                                </td>
                                <td class="py-2 text-center text-2xl">@item.Fecha</td>
                                <td class="py-2 text-center text-2xl">@item.Acta</td>
                                <td class="py-2 text-center text-2xl">@item.Inmueble</td>
                                <td class="py-2 text-center text-2xl">@item.Habitaciones</td>
                                <td class="py-2 text-center text-2xl">@item.Arrendador</td>
                                <td class="py-2 text-center text-2xl">@item.Arrendatario</td>
                                <td class="py-2 text-center text-2xl">@item.Delegacion</td>
                                <td class="py-2 text-center text-2xl">@item.Agente</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
            <div class="absolute bottom-0 left-0 right-0 flex justify-center">
                <span class="flex border-3 rounded-lg bg-slate-100">
                    <button class="py-2 my-1 mx-2 rounded-lg text-blue-900 flex justify-center">
                        <g class="flex font-semibold">
                            <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-arrow-left-circle-fill self-center pr-1" viewBox="0 0 16 16">
                                <path d="M8 0a8 8 0 1 0 0 16A8 8 0 0 0 8 0zm3.5 7.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5z" />
                            </svg>
                            Primera
                        </g>
                    </button>
                    <button class="py-2 my-1 mx-2 rounded-lg text-blue-900 flex justify-center">
                        <g class="flex">
                            Anterior
                        </g>
                    </button>
                    <button class="py-2 my-1 mx-2 w-12 h-12 rounded-lg text-blue-900 border-2 border-blue-900 text-blue-900 flex justify-center" style="background-color: #E3F2F6;">
                        <g class="flex">
                            1
                        </g>
                    </button>
                    <button class="py-2 my-1 mx-2 w-12 h-12 rounded-lg text-blue-900 border-2 border-blue-900 text-blue-900 flex justify-center">
                        <g class="flex">
                            2
                        </g>
                    </button>
                    <button class="py-2 my-1 mx-2 rounded-lg text-blue-900 flex justify-center">
                        <g class="flex">
                            Siguiente
                        </g>
                    </button>
                    <button class="py-2 my-1 mx-2 rounded-lg text-blue-900 flex justify-center">
                        <g class="flex font-semibold">
                            Última
                            <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-arrow-right-circle-fill self-center pl-1" viewBox="0 0 16 16">
                                <path d="M8 0a8 8 0 1 1 0 16A8 8 0 0 1 8 0zM4.5 7.5a.5.5 0 0 0 0 1h5.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3a.5.5 0 0 0 0-.708l-3-3a.5.5 0 1 0-.708.708L10.293 7.5H4.5z" />
                            </svg>
                        </g>
                    </button>
                </span>
            </div>
        </div>
    </div>
</div>